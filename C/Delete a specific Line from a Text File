#include <stdio.h>
/* Include other headers as needed */
int main()
{
	char a[20], ch;
  	int c, line, count;
  	FILE *fp;
  	scanf("%s", a);
  	scanf("%d", &c);
  	fp = fopen(a, "r");
  	for(int i=0;i<c;i++)
    {
      count=0;
      scanf("%d", &line);
	  ch = fgetc(fp);
      while(!feof(fp))
      {
        if(ch == '\n')
        {
          count++;
        }
        ch = fgetc(fp);
      }
      if(line > count)
      {
        printf("\n%d", -1);
        rewind(fp);
      }
      else
      {
        rewind(fp);
        count=1;
        ch = fgetc(fp);
        while(!feof(fp))
        {
          if(ch == '\n')
          {
            count++;
          }
          if(count != line)
          {
            printf("%c", ch);
          }
          ch = fgetc(fp);
        }
      }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
